   "source": [
    "#### (c) Create a matrix with 1/3's in the diagonals, using diag() and ones()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "id": "0ddeee1d",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "[[0.33333333 0.33333333 0.         0.         0.        ]\n",
      " [0.33333333 0.33333333 0.33333333 0.         0.        ]\n",
      " [0.         0.33333333 0.33333333 0.33333333 0.        ]\n",
      " [0.         0.         0.33333333 0.33333333 0.33333333]\n",
      " [0.         0.         0.         0.33333333 0.33333333]]\n"
     ]
    }
   ],
   "source": [
    "def thirdsDiag(n): # Returns a n*n matrix with 1/3 in the diagonal\n",
    "    # Create 2 vectors with all ones.\n",
    "    v_1 = np.ones((1,n-1)) # Remove one to fit in the matrix\n",
    "    v_2 = np.ones((1,n  ))\n",
    "\n",
    "    # Make the vectors into diagonals\n",
    "    r_1 = np.diag(v_1[0], 1)\n",
    "    r_2 = np.diag(v_2[0], 0)\n",
    "    r_3 = np.diag(v_1[0], -1)\n",
    "    \n",
    "    # Add the diagonals together and perform scalar multiplication\n",
    "    return (r_1+r_2+r_3)*(1/3)\n",
    "\n",
    "print(thirdsDiag(5))"
   ]
  },
